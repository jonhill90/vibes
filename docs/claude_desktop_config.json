{
  "mcpServers": {
    "agentbox": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "http://localhost:8054/mcp"
      ]
    },
    "basic-memory": {
      "command": "docker",
      "args": ["exec", "-i", "basic-memory-mcp", "/app/start.sh"]
    },
    "MCP_DOCKER": {
      "command": "docker",
      "args": ["mcp", "gateway", "run"]
    }
  }
}