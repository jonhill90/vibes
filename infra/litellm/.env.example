# Database Connection (URL-encoded password)
# Format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://litellm_user:YOUR_ENCODED_PASSWORD@postgres:5432/litellm
STORE_MODEL_IN_DB=True

# LiteLLM Master Key
# Generate with: openssl rand -hex 32
LITELLM_MASTER_KEY=your_master_key_here

# UI Configuration  
UI_USERNAME=admin
UI_PASSWORD=your_encoded_ui_password_here
UI_BASE_URL=http://localhost:4000

# Provider API Keys
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
GROQ_API_KEY=gsk_your-groq-key
GEMINI_API_KEY=your-gemini-key

# Logging
LITELLM_LOG=INFO
LINEAR_API_KEY=your_linear_api_key_here