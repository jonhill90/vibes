# PostgreSQL Master Credentials
# These are the superuser credentials for the entire PostgreSQL instance
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_postgres_password_here
POSTGRES_DB=postgres

# Application-Specific Databases
# Add additional database configurations as needed for different applications
# Example: LiteLLM Database Configuration
LITELLM_DB_NAME=litellm
LITELLM_DB_USER=litellm_user
LITELLM_DB_PASSWORD=your_litellm_db_password_here

# You can add more application databases here:
# MYAPP_DB_NAME=myapp
# MYAPP_DB_USER=myapp_user
# MYAPP_DB_PASSWORD=your_myapp_password

# Port Configuration
POSTGRES_HOST=127.0.0.1
POSTGRES_PORT=5432
